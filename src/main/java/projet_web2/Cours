package projet_web2;

public class Cours {
	private String nom_UE;
	private int salle;
	
	public Cours(String nom_UE) {
		super();
		this.nom_UE = nom_UE;
	}
	public Cours(String nom_UE, int salle) {
		super();
		this.nom_UE = nom_UE;
		this.setSalle(salle);
	}
	public String getNom_UE() {
		return nom_UE;
	}
	public void setNom_UE(String nom_UE) {
		this.nom_UE = nom_UE;
	}
	public int getSalle() {
		return salle;
	}
	public void setSalle(int salle) {
		this.salle = salle;
	}
}
